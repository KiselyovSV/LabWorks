@model IEnumerable<MvcCreditApp1.Models.Bid>

<div id="results">
    <h4> Все заявители кредита "@Model.First().CreditHead"</h4>
    <ul>
        @foreach (var item in Model)
        {
            <li>@item.Name</li>
        }
    </ul>
    <ul class="navbar-nav flex-grow-1">
        <li class="nav-item">
            <a asp-area="" asp-controller="Home" asp-action="GetJsonFile">Скачать в формате json</a>
        </li>
    </ul>
    <ul class="navbar-nav flex-grow-1">
        <li class="nav-item">
            <a asp-area="" asp-controller="Home" asp-action="GetTxtFile">Скачать в формате txt</a>
        </li>
    </ul>
</div>